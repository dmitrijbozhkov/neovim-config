		"s": "nodeNext",
		"<cr>": ["expandable?", "expand", "open"],
		"t": "open:tab",
		"v": "open:vsplit",
		"h": "open:split",
		"b": "collapse",
		"B": "collapseRecursive",
		"p": "cd",
		"l": "search",
		"L": "searchRecursive",
		"e": "copyFile",
		"E": "copyFilePath",
		"r": "delete",
		"R": "deleteForewer",
		"x": "cutFile",
		"q": "pasteFile",
		"f": "addFile",
		"F": "addDirectory",
		"n": "rename",
		"U": "refresh",
